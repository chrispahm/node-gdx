{
  "name": "node-gdx",
  "version": "2.0.1",
  "description": "Node.js bindings for GAMS GDX containers.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/chrispahm/node-gdx",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "preinstall": "npm i node-addon-api",
    "test": "node test/test.js",
    "clean": "node-gyp clean",
    "build": "node-gyp rebuild --openssl_fips=''",
    "install": "node-gyp-build",
    "prebuild": "prebuildify --napi -t 12.0.0 --strip --openssl_fips=''"
  },
  "keywords": [
    "GDX",
    "GAMS",
    "Optimisation"
  ],
  "author": "Christoph Pahmeyer",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.6.0"
  },
  "devDependencies": {
    "node-gyp": "^9.3.1",
    "prebuildify": "^5.0.1"
  }
}
